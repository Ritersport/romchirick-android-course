<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <style name="RoundedRectangle">
        <item name="cornerSize">8dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

    
    <style name="HeadlineText" parent="@style/TextAppearance.Material3.TitleMedium">
        <item name="android:lineSpacingExtra">5sp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#E2E2E2</item>
    </style>

    <style name="SupportingText" parent="@style/TextAppearance.Material3.BodyMedium">
        <item name="android:gravity">top</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#C3C7C7</item>
    </style>

    <style name="Overline" parent="TextAppearance.Material3.LabelMedium">
        <item name="android:fontWeight">500</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#91D2D5</item>
        <item name="android:lineSpacingExtra">2sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:letterSpacing">0.04</item>
    </style>
</resources>